<?xml version="1.0"?>
<!DOCTYPE smooks-resource-list PUBLIC "-//MILYN//DTD SMOOKS 1.0//EN" "http://www.milyn.org/dtd/smooksres-list-1.0.dtd">

<smooks-resource-list> 

	<!--
		Configure the bean creation and population...
	-->
	<smooks-resource selector="c" path="org.milyn.javabean.ProcessingPhaseBeanPopulator">
		<param name="beanId">myBeanData</param>
		<param name="beanClass">org.milyn.templating.stringtemplate.MyBean</param>
		<param name="attributeName">x</param>
