<?xml version="1.0"?>
<!DOCTYPE smooks-resource-list PUBLIC "-//MILYN//DTD SMOOKS 2.0//EN" "http://www.milyn.org/dtd/smooksres-list-2.0.dtd">

<!--
	This cdrl config defines the default Content Delivery Unit Creators
	installed in Smooks.
-->

<smooks-resource-list default-selector="cdu-creator"> 

	<resource-config>
	    <resource type="class">org.milyn.delivery.JavaContentDeliveryUnitCreator</resource>
		<param name="restype">class</param>
	</resource-config>
	
</smooks-resource-list>
