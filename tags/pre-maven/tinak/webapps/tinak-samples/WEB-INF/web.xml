<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <display-name>Tinak Sample</display-name>
    <description>Tinak sample JSP pages.</description>

	<!-- Tinak Tag Library Descriptor -->
	<taglib>
		<taglib-uri>www.milyn.org/tld/tinak.tld</taglib-uri>
		<taglib-location>/WEB-INF/lib/milyn-tinak-taglib-0.6.jar</taglib-location>
	</taglib>

	<!-- Template Tag Library Descriptor -->
	<taglib>
		<taglib-uri>/WEB-INF/struts-template.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-template.tld</taglib-location>
	</taglib>	    
</web-app>
