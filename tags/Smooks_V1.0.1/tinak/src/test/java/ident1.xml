<?xml version="1.0"?>
<!DOCTYPE device-ident PUBLIC "-//MILYN//DTD TINAK 1.0//EN" "http://www.milyn.org/dtd/device-ident-1.0.dtd">

<device-ident>
    <device name="Nokia 7210">
        <http-req-param name="ua" value="Nokia 7210" />
    </device>
    <device name="www" match="*">
        <http-req-header name="User-Agent" value="xxx"/>
        <http-req-param name="ua" value="xxx" />
    </device>
    <device name="uuu" match="all">
        <http-req-param id="1" name="ua" value="uuu" />
        <http-req-header name="User-Agent" value="uuu"/>
    </device>
    <device name="vvv" match="any">
        <http-req-param id="1" name="ua" value="vvv" />
        <http-req-header name="User-Agent" value="vvv"/>
    </device>
    <device name="xxx" match="1|2">
        <http-req-param id="1" name="ua" value="xxx" />
        <http-req-header name="User-Agent" value="xxx"/>
    </device>
    <device name="yyy">
        <http-req-header name="User-Agent" value="yyy" />
        <http-req-header name="ua" value="xxx" />
    </device>
    <device name="zzz">
        <http-req-param name="User-Agent" value="zzz" />
        <http-req-param name="ua" value="zzz" />
    </device>
</device-ident>


